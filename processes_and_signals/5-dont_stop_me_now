#!/usr/bin/env bash
# This script finds and kills the 4-to_infinity_and_beyond process

pid=$(ps -eo pid,cmd | grep '[4]-to_infinity_and_beyond' | awk '{print $1}')
if [ -n "$pid" ]; then
  kill "$pid"
  echo "Process 4-to_infinity_and_beyond with PID $pid has been killed."
else
  echo "Process 4-to_infinity_and_beyond not found."
fi

